<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgound_fcfcfc"
    android:orientation="vertical"
    >

    <!--<com.yadong.huawei.ui.widget.recyclerview.layoutmanager.PullRecyclerViewGroup-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="match_parent"-->
        <!--&gt;-->

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <include
                    layout="@layout/userinfo_my"/>

                <GridView
                    android:id="@+id/gv_title_grid"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/ui_10_dp"
                    android:layout_marginLeft="@dimen/ui_16_dp"
                    android:layout_marginRight="@dimen/ui_16_dp"
                    android:layout_marginTop="20dp"
                    android:stretchMode="columnWidth"/>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="8.0dp"
                    android:background="@color/black_alpha_03"/>

                <com.yadong.huawei.module.widget.EnterLayout
                    android:id="@+id/book_game_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="?android:attr/selectableItemBackground"/>

                <com.yadong.huawei.module.widget.EnterLayout
                    android:id="@+id/buy_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="?android:attr/selectableItemBackground"/>

                <com.yadong.huawei.module.widget.EnterLayout
                    android:id="@+id/huaban_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="?android:attr/selectableItemBackground"/>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="8.0dp"
                    android:background="@color/black_alpha_03"/>


                <com.yadong.huawei.module.widget.EnterLayout
                    android:id="@+id/setting_computer"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="?android:attr/selectableItemBackground"

                    />

                <com.yadong.huawei.module.widget.EnterLayout
                    android:id="@+id/faq_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="?android:attr/selectableItemBackground"/>

                <com.yadong.huawei.module.widget.EnterLayout
                    android:id="@+id/check_update_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="?android:attr/selectableItemBackground"/>

                <com.yadong.huawei.module.widget.EnterLayout
                    android:id="@+id/about_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="?android:attr/selectableItemBackground"/>
            </LinearLayout>
        </ScrollView>
    <!--</com.yadong.huawei.ui.widget.recyclerview.layoutmanager.PullRecyclerViewGroup>-->
</LinearLayout>
